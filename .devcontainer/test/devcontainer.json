{
  "name": "test",
  "workspaceFolder": "/home/jovyan",
  "image": "ghcr.io/nmfs-opensci/container-images/py-rocket-base:latest",
  "hostRequirements": {
    "cpus": "2"
  },
  "customizations": {
    "codespaces": {
      "openFiles": [
        ".gitconfig",
        "../../workspaces/container-images/.devcontainer/codespace/README.md"
      ]
    }
  },
  "portsAttributes": {
    "8889": {
      "label": "Jupyter Lab",
      "onAutoForward": "openBrowser"
    }
  },
  "postStartCommand": "echo 'abc' > README.md && jupyter lab --ip=0.0.0.0 --port=8889 --allow-root --no-browser --NotebookApp.token='' --NotebookApp.password=''"
}